<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Version>1.1.3</Version>
    <PackageReleaseNotes>support for deployment subscriptions added. SourceLink is now supported.</PackageReleaseNotes>

    <TargetFramework>netstandard2.0</TargetFramework>
    
    <Authors>Oisin Haken</Authors>
    <Company>Eshopworld</Company>
    <Copyright>Eshopworld</Copyright>
    <PackageProjectUrl>https://github.com/eShopWorld/devops</PackageProjectUrl>
    <RepositoryUrl>https://github.com/eShopWorld/devops</RepositoryUrl>
    <PackageTags>configuration, devops</PackageTags>
    <PackageLicenseUrl>https://github.com/eShopWorld/devops/blob/master/LICENSE</PackageLicenseUrl>
    <Description>eShopWorld DevOps SDK artefacts</Description>
    <DebugType>portable</DebugType>
    <LangVersion>latest</LangVersion>
    <IncludeSource>True</IncludeSource>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Eshopworld.Core" Version="2.0.0" />
    <PackageReference Include="Microsoft.Azure.KeyVault" Version="3.0.0" />
    <PackageReference Include="Microsoft.Extensions.Configuration.AzureKeyVault" Version="2.1.1" />
    <PackageReference Include="Microsoft.Extensions.Configuration.EnvironmentVariables" Version="2.1.1" />
    <PackageReference Include="Microsoft.Extensions.Configuration.Json" Version="2.1.1" />
  </ItemGroup>

</Project>
