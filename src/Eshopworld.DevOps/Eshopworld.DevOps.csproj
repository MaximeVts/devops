<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>

    <TargetFramework>netstandard2.0</TargetFramework>
    
    <Authors>Oisin Haken</Authors>
    <Company>Eshopworld</Company>
    <Copyright>Eshopworld</Copyright>
    <PackageProjectUrl>https://github.com/eShopWorld/devops</PackageProjectUrl>
    <RepositoryUrl>https://github.com/eShopWorld/devops</RepositoryUrl>
    <PackageTags>configuration, devops</PackageTags>
    <PackageReleaseNotes>initial release with core configuration artefacts</PackageReleaseNotes>
    <Version>1.0.0</Version>
    <PackageLicenseUrl>https://github.com/eShopWorld/devops/blob/master/LICENSE</PackageLicenseUrl>
    <Description>eShopWorld DevOps SDK artefacts</Description>
    <DebugType>Full</DebugType>
    <LangVersion>latest</LangVersion>
    <IncludeSource>True</IncludeSource>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Eshopworld.Core" Version="1.0.1" />
    <PackageReference Include="Microsoft.Extensions.Configuration.AzureKeyVault" Version="2.0.1" />
    <PackageReference Include="Microsoft.Extensions.Configuration.EnvironmentVariables" Version="2.0.1" />
    <PackageReference Include="Microsoft.Extensions.Configuration.Json" Version="2.0.1" />
  </ItemGroup>

</Project>
